          <h3 style="margin-bottom: 1rem;">–ü—Ä–µ–¥–ø—Ä–æ—Å–º–æ—Ç—Ä</h3>
          <p>–¢–∞–∫ –±—É–¥–µ—Ç –≤—ã–≥–ª—è–¥–µ—Ç—å –≤–∞—à –∞—Å—Å–∏—Å—Ç–µ–Ω—Ç –Ω–∞ —Å–∞–π—Ç–µ:</p>
          
          <div style="margin-top: 1.5rem; text-align: center;">
            <div style="display: inline-block; position: relative;">
              <div style="width: 400px; height: 300px; background-color: #f3f4f6; border: 1px dashed #d1d5db; border-radius: 0.5rem; display: flex; justify-content: center; align-items: center;">
                <div style="position: absolute; bottom: 20px; right: 20px; width: 60px; height: 60px; border-radius: 50%; background: linear-gradient(135deg, #4a86e8, #2563eb); display: flex; justify-content: center; align-items: center; color: white; box-shadow: 0 4px 15px rgba(74, 134, 232, 0.4);">
                  <i class="fas fa-robot" style="font-size: 1.5rem;"></i>
                </div>
              </div>
              <div style="margin-top: 0.5rem; font-size: 0.875rem; color: var(--text-gray);">–ü—Ä–∏–º–µ—Ä –≤–∞—à–µ–≥–æ —Å–∞–π—Ç–∞ —Å –≤–∏–¥–∂–µ—Ç–æ–º –≥–æ–ª–æ—Å–æ–≤–æ–≥–æ –∞—Å—Å–∏—Å—Ç–µ–Ω—Ç–∞</div>
            </div>
          </div>
        </div>
      </div>
    </div>
    
    <!-- –£–≤–µ–¥–æ–º–ª–µ–Ω–∏–µ -->
    <div class="notification notification-success" id="notification" style="display: none;">
      <div class="notification-icon">
        <i class="fas fa-check-circle"></i>
      </div>
      <div class="notification-content">
        <div id="notification-message">–û–ø–µ—Ä–∞—Ü–∏—è —É—Å–ø–µ—à–Ω–æ –≤—ã–ø–æ–ª–Ω–µ–Ω–∞!</div>
      </div>
      <button class="notification-close" id="notification-close">&times;</button>
    </div>
  </main>
  
  <script>
    document.addEventListener('DOMContentLoaded', function() {
      // –û–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–µ –ø–µ—Ä–µ–º–µ–Ω–Ω—ã—Ö
      let currentAgentId = null;
      
      // –ü–æ–ª—É—á–∞–µ–º —ç–ª–µ–º–µ–Ω—Ç—ã
      const tabs = document.querySelectorAll('.tab');
      const tabContents = document.querySelectorAll('.tab-content');
      const voiceOptions = document.querySelectorAll('.voice-option');
      const agentForm = document.getElementById('agent-form');
      const viewEmbedCodeBtn = document.getElementById('view-embed-code');
      const deleteAgentBtn = document.getElementById('delete-agent');
      const saveAgentBtn = document.getElementById('save-agent');
      const cancelButton = document.getElementById('cancel-button');
      const copyEmbedCodeBtn = document.getElementById('copy-embed-code');
      const logoutButton = document.getElementById('logout-button');
      
      // –û—Å–Ω–æ–≤–Ω—ã–µ —Ñ—É–Ω–∫—Ü–∏–∏ –¥–ª—è —Ä–∞–±–æ—Ç—ã —Å API
      const api = {
        baseUrl: '/api',
        
        // –ü–æ–ª—É—á–µ–Ω–∏–µ —Ç–æ–∫–µ–Ω–∞ –∞–≤—Ç–æ—Ä–∏–∑–∞—Ü–∏–∏
        getToken() {
          return localStorage.getItem('auth_token');
        },
        
        // –ü—Ä–æ–≤–µ—Ä–∫–∞ –∞–≤—Ç–æ—Ä–∏–∑–∞—Ü–∏–∏
        isAuthenticated() {
          return this.getToken() !== null;
        },
        
        // –ë–∞–∑–æ–≤–∞—è —Ñ—É–Ω–∫—Ü–∏—è –¥–ª—è –∑–∞–ø—Ä–æ—Å–æ–≤ –∫ API
        async fetch(endpoint, options = {}) {
          if (this.isAuthenticated()) {
            options.headers = {
              ...options.headers,
              'Authorization': `Bearer ${this.getToken()}`
            };
          }
          
          if (options.body && typeof options.body !== 'string') {
            options.headers = {
              ...options.headers,
              'Content-Type': 'application/json'
            };
            options.body = JSON.stringify(options.body);
          }
          
          try {
            const response = await fetch(`${this.baseUrl}${endpoint}`, options);
            
            if (response.status === 401) {
              localStorage.removeItem('auth_token');
              window.location.href = '/static/login.html';
              throw new Error('–¢—Ä–µ–±—É–µ—Ç—Å—è –∞–≤—Ç–æ—Ä–∏–∑–∞—Ü–∏—è');
            }
            
            const data = await response.json();
            
            if (!response.ok) {
              throw new Error(data.detail || '–ü—Ä–æ–∏–∑–æ—à–ª–∞ –æ—à–∏–±–∫–∞ –ø—Ä–∏ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏–∏ –∑–∞–ø—Ä–æ—Å–∞');
            }
            
            return data;
          } catch (error) {
            console.error('API Error:', error);
            throw error;
          }
        },
        
        // –ú–µ—Ç–æ–¥—ã –¥–ª—è —Ä–∞–∑–ª–∏—á–Ω—ã—Ö —Ç–∏–ø–æ–≤ –∑–∞–ø—Ä–æ—Å–æ–≤
        get(endpoint) {
          return this.fetch(endpoint, { method: 'GET' });
        },
        
        post(endpoint, data) {
          return this.fetch(endpoint, { method: 'POST', body: data });
        },
        
        put(endpoint, data) {
          return this.fetch(endpoint, { method: 'PUT', body: data });
        },
        
        delete(endpoint) {
          return this.fetch(endpoint, { method: 'DELETE' });
        },
        
        // –ü–æ–ª—É—á–µ–Ω–∏–µ –∞–≥–µ–Ω—Ç–∞ –ø–æ ID
        getAgent(id) {
          return this.get(`/assistants/${id}`);
        },
        
        // –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ –∞–≥–µ–Ω—Ç–∞
        updateAgent(id, data) {
          return this.put(`/assistants/${id}`, data);
        },
        
        // –£–¥–∞–ª–µ–Ω–∏–µ –∞–≥–µ–Ω—Ç–∞
        deleteAgent(id) {
          return this.delete(`/assistants/${id}`);
        },
        
        // –ü–æ–ª—É—á–µ–Ω–∏–µ –∫–æ–¥–∞ –≤—Å—Ç—Ä–∞–∏–≤–∞–Ω–∏—è
        getEmbedCode(id) {
          return this.get(`/assistants/${id}/embed-code`);
        }
      };
      
      // –í—Å–ø–æ–º–æ–≥–∞—Ç–µ–ª—å–Ω—ã–µ —Ñ—É–Ω–∫—Ü–∏–∏ –¥–ª—è UI
      const ui = {
        // –ü–æ–∫–∞–∑–∞—Ç—å —É–≤–µ–¥–æ–º–ª–µ–Ω–∏–µ
        showNotification(message, type = 'success') {
          const notification = document.getElementById('notification');
          const notificationMessage = document.getElementById('notification-message');
          
          // –£–¥–∞–ª—è–µ–º –≤—Å–µ –∫–ª–∞—Å—Å—ã —Ç–∏–ø–æ–≤
          notification.classList.remove('notification-success', 'notification-error', 'notification-info');
          
          // –£—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º —Ç–∏–ø –∏ —Å–æ–æ–±—â–µ–Ω–∏–µ
          notification.classList.add(`notification-${type}`);
          notificationMessage.textContent = message;
          
          // –ü–æ–∫–∞–∑—ã–≤–∞–µ–º —É–≤–µ–¥–æ–º–ª–µ–Ω–∏–µ
          notification.style.display = 'flex';
          setTimeout(() => {
            notification.classList.add('show');
          }, 10);
          
          // –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ —Å–∫—Ä—ã–≤–∞–µ–º —á–µ—Ä–µ–∑ 5 —Å–µ–∫—É–Ω–¥
          setTimeout(() => {
            this.hideNotification();
          }, 5000);
        },
        
        // –°–∫—Ä—ã—Ç—å —É–≤–µ–¥–æ–º–ª–µ–Ω–∏–µ
        hideNotification() {
          const notification = document.getElementById('notification');
          notification.classList.remove('show');
          setTimeout(() => {
            notification.style.display = 'none';
          }, 300);
        },
        
        // –ü–µ—Ä–µ–∫–ª—é—á–µ–Ω–∏–µ –≤–∫–ª–∞–¥–æ–∫
        switchTab(tabId) {
          tabs.forEach(tab => {
            tab.classList.toggle('active', tab.getAttribute('data-tab') === tabId);
          });
          
          tabContents.forEach(content => {
            content.classList.toggle('active', content.id === `${tabId}-tab`);
          });
        },
        
        // –ó–∞–ø–æ–ª–Ω–µ–Ω–∏–µ —Ñ–æ—Ä–º—ã –¥–∞–Ω–Ω—ã–º–∏ –∞–≥–µ–Ω—Ç–∞
        fillAgentForm(agent) {
          document.getElementById('agent-name').value = agent.name || '';
          document.getElementById('agent-description').value = agent.description || '';
          document.getElementById('system-prompt').value = agent.system_prompt || '';
          
          // –í—ã–±–æ—Ä –≥–æ–ª–æ—Å–∞
          const voiceInput = document.querySelector(`input[name="voice"][value="${agent.voice}"]`);
          if (voiceInput) {
            voiceOptions.forEach(option => {
              option.classList.remove('selected');
            });
            voiceInput.closest('.voice-option').classList.add('selected');
            voiceInput.checked = true;
          }
        },
        
        // –ü–æ–ª—É—á–µ–Ω–∏–µ –¥–∞–Ω–Ω—ã—Ö –∏–∑ —Ñ–æ—Ä–º—ã
        getFormData() {
          return {
            name: document.getElementById('agent-name').value,
            description: document.getElementById('agent-description').value,
            system_prompt: document.getElementById('system-prompt').value,
            voice: document.querySelector('input[name="voice"]:checked').value
          };
        }
      };
      
      // –ü—Ä–æ–≤–µ—Ä–∫–∞ –∞–≤—Ç–æ—Ä–∏–∑–∞—Ü–∏–∏
      if (!api.isAuthenticated()) {
        window.location.href = '/static/login.html';
        return;
      }
      
      // –ü–æ–ª—É—á–µ–Ω–∏–µ ID –∞–≥–µ–Ω—Ç–∞ –∏–∑ URL –ø–∞—Ä–∞–º–µ—Ç—Ä–æ–≤
      const urlParams = new URLSearchParams(window.location.search);
      currentAgentId = urlParams.get('id');
      
      // –ï—Å–ª–∏ ID –Ω–µ —É–∫–∞–∑–∞–Ω, –ø–µ—Ä–µ–Ω–∞–ø—Ä–∞–≤–ª—è–µ–º –Ω–∞ —Å—Ç—Ä–∞–Ω–∏—Ü—É —Å–æ —Å–ø–∏—Å–∫–æ–º –∞–≥–µ–Ω—Ç–æ–≤
      if (!currentAgentId) {
        window.location.href = '/static/dashboard.html';
        return;
      }
      
      // –ó–∞–≥—Ä—É–∑–∫–∞ –¥–∞–Ω–Ω—ã—Ö –∞–≥–µ–Ω—Ç–∞
      async function loadAgentData() {
        try {
          const agent = await api.getAgent(currentAgentId);
          ui.fillAgentForm(agent);
          
          // –ó–∞–≥—Ä—É–∂–∞–µ–º –∫–æ–¥ –≤—Å—Ç—Ä–∞–∏–≤–∞–Ω–∏—è
          const embedData = await api.getEmbedCode(currentAgentId);
          document.getElementById('embed-code').textContent = embedData.embed_code;
        } catch (error) {
          ui.showNotification(error.message || '–û—à–∏–±–∫–∞ –ø—Ä–∏ –∑–∞–≥—Ä—É–∑–∫–µ –¥–∞–Ω–Ω—ã—Ö –∞–≥–µ–Ω—Ç–∞', 'error');
        }
      }
      
      // –û–±—Ä–∞–±–æ—Ç—á–∏–∫–∏ —Å–æ–±—ã—Ç–∏–π
      
      // –ü–µ—Ä–µ–∫–ª—é—á–µ–Ω–∏–µ –≤–∫–ª–∞–¥–æ–∫
      tabs.forEach(tab => {
        tab.addEventListener('click', () => {
          const tabId = tab.getAttribute('data-tab');
          ui.switchTab(tabId);
        });
      });
      
      // –í—ã–±–æ—Ä –≥–æ–ª–æ—Å–∞
      voiceOptions.forEach(option => {
        option.addEventListener('click', function() {
          voiceOptions.forEach(opt => {
            opt.classList.remove('selected');
          });
          this.classList.add('selected');
          this.querySelector('input').checked = true;
        });
      });
      
      // –ö–Ω–æ–ø–∫–∞ "–ö–æ–¥ –¥–ª—è –≤—Å—Ç—Ä–∞–∏–≤–∞–Ω–∏—è"
      viewEmbedCodeBtn.addEventListener('click', () => {
        ui.switchTab('embed');
      });
      
      // –ö–æ–ø–∏—Ä–æ–≤–∞–Ω–∏–µ –∫–æ–¥–∞ –≤—Å—Ç—Ä–∞–∏–≤–∞–Ω–∏—è
      copyEmbedCodeBtn.addEventListener('click', () => {
        const embedCode = document.getElementById('embed-code');
        const tempTextarea = document.createElement('textarea');
        tempTextarea.value = embedCode.textContent;
        document.body.appendChild(tempTextarea);
        tempTextarea.select();
        document.execCommand('copy');
        document.body.removeChild(tempTextarea);
        
        ui.showNotification('–ö–æ–¥ –¥–ª—è –≤—Å—Ç—Ä–∞–∏–≤–∞–Ω–∏—è —Å–∫–æ–ø–∏—Ä–æ–≤–∞–Ω!', 'success');
      });
      
      // –°–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ –∏–∑–º–µ–Ω–µ–Ω–∏–π (–∫–Ω–æ–ø–∫–∞ –≤ –≤–µ—Ä—Ö–Ω–µ–π –ø–∞–Ω–µ–ª–∏)
      saveAgentBtn.addEventListener('click', async () => {
        try {
          const formData = ui.getFormData();
          await api.updateAgent(currentAgentId, formData);
          ui.showNotification('–ò–∑–º–µ–Ω–µ–Ω–∏—è —É—Å–ø–µ—à–Ω–æ —Å–æ—Ö—Ä–∞–Ω–µ–Ω—ã!', 'success');
        } catch (error) {
          ui.showNotification(error.message || '–û—à–∏–±–∫–∞ –ø—Ä–∏ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏–∏ –∏–∑–º–µ–Ω–µ–Ω–∏–π', 'error');
        }
      });
      
      // –û—Ç–ø—Ä–∞–≤–∫–∞ —Ñ–æ—Ä–º—ã
      agentForm.addEventListener('submit', async (e) => {
        e.preventDefault();
        
        try {
          const formData = ui.getFormData();
          await api.updateAgent(currentAgentId, formData);
          ui.showNotification('–ò–∑–º–µ–Ω–µ–Ω–∏—è —É—Å–ø–µ—à–Ω–æ —Å–æ—Ö—Ä–∞–Ω–µ–Ω—ã!', 'success');
        } catch (error) {
          ui.showNotification(error.message || '–û—à–∏–±–∫–∞ –ø—Ä–∏ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏–∏ –∏–∑–º–µ–Ω–µ–Ω–∏–π', 'error');
        }
      });
      
      // –û—Ç–º–µ–Ω–∞ –∏–∑–º–µ–Ω–µ–Ω–∏–π
      cancelButton.addEventListener('click', () => {
        // –ü–µ—Ä–µ–∑–∞–≥—Ä—É–∂–∞–µ–º –¥–∞–Ω–Ω—ã–µ –∞–≥–µ–Ω—Ç–∞
        loadAgentData();
        ui.showNotification('–ò–∑–º–µ–Ω–µ–Ω–∏—è –æ—Ç–º–µ–Ω–µ–Ω—ã', 'info');
      });
      
      // –£–¥–∞–ª–µ–Ω–∏–µ –∞–≥–µ–Ω—Ç–∞
      deleteAgentBtn.addEventListener('click', async () => {
        const confirmed = confirm('–í—ã —É–≤–µ—Ä–µ–Ω—ã, —á—Ç–æ —Ö–æ—Ç–∏—Ç–µ —É–¥–∞–ª–∏—Ç—å —ç—Ç–æ–≥–æ –∞–≥–µ–Ω—Ç–∞? –≠—Ç–æ –¥–µ–π—Å—Ç–≤–∏–µ –Ω–µ–ª—å–∑—è –æ—Ç–º–µ–Ω–∏—Ç—å.');
        
        if (confirmed) {
          try {
            await api.deleteAgent(currentAgentId);
            ui.showNotification('–ê–≥–µ–Ω—Ç —É—Å–ø–µ—à–Ω–æ —É–¥–∞–ª–µ–Ω!', 'success');
            
            // –ü–µ—Ä–µ–Ω–∞–ø—Ä–∞–≤–ª—è–µ–º –Ω–∞ —Å—Ç—Ä–∞–Ω–∏—Ü—É —Å–æ —Å–ø–∏—Å–∫–æ–º –∞–≥–µ–Ω—Ç–æ–≤
            setTimeout(() => {
              window.location.href = '/static/dashboard.html';
            }, 1000);
          } catch (error) {
            ui.showNotification(error.message || '–û—à–∏–±–∫–∞ –ø—Ä–∏ —É–¥–∞–ª–µ–Ω–∏–∏ –∞–≥–µ–Ω—Ç–∞', 'error');
          }
        }
      });
      
      // –ó–∞–∫—Ä—ã—Ç–∏–µ —É–≤–µ–¥–æ–º–ª–µ–Ω–∏—è
      document.getElementById('notification-close').addEventListener('click', () => {
        ui.hideNotification();
      });
      
      // –í—ã—Ö–æ–¥ –∏–∑ —Å–∏—Å—Ç–µ–º—ã
      logoutButton.addEventListener('click', () => {
        localStorage.removeItem('auth_token');
        window.location.href = '/static/login.html';
      });
      
      // –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è —Å—Ç—Ä–∞–Ω–∏—Ü—ã
      loadAgentData();
    });
  </script>
</body>
</html>
